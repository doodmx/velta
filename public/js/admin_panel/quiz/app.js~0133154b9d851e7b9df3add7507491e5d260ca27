function computeOptionsCredits() {
  _instance__WEBPACK_IMPORTED_MODULE_10__["default"].questions.forEach(function (question) {
    var questionType = question.type;
    var rightOptions = question.options.filter(function (option) {
      return option.is_right_one;
    }).length;
    var optionCredits = question.credits;

    if (questionType === 'checkbox') {
      optionCredits = (question.credits / rightOptions).toFixed(2);
    }

    question.options.forEach(function (option, optionIndex) {
      if (option.is_right_one === 'true') {
        option.setIsRightOne(true);
      }

      if (option.is_right_one) {
        option.setCredits(optionCredits);
      }
    });
  });
}
